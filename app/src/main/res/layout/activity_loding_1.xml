<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAF1E2"
    tools:context=".loding">


    <TextView
        android:id="@+id/text_loding_title"
        android:layout_width="217dp"
        android:layout_height="57dp"
        android:fontFamily="@font/sanghu_dajungdagam"
        android:text="   Mapping"
        android:textColor="#5D5D5D"
        android:textSize="50dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="160dp" />

    <TextView
        android:id="@+id/text_loding_underbar"
        android:layout_width="121dp"
        android:layout_height="34dp"
        android:fontFamily="@font/sanghu_dajungdagam"
        android:text="시작하려면 사진을 터치해주세요"
        android:textColor="#5D5D5D"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="510dp" />

    <ImageView
        android:id="@+id/text_loding_image"
        android:layout_width="243dp"
        android:layout_height="280dp"
        android:layout_gravity="center"
        android:contentDescription="@string/logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/loding"
        tools:layout_editor_absoluteY="217dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/text_loding_title1"
        android:layout_width="236dp"
        android:layout_height="66dp"
        android:fontFamily="@font/sanghu_dajungdagam"
        android:text="함께 쓰는 일기"
        android:textColor="#5D5D5D"
        android:textSize="50dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="87dp" />



</androidx.constraintlayout.widget.ConstraintLayout>